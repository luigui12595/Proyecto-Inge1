
@model ProyectoInge1.Models.ModUsuarioInter
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr><th>Nombre Completo</th><th>Rol</th><th>Proyecto</th></tr>
    </thead>
    <tbody class="rowlink" data-link = "row" data-target="Index">
        @for (int i = 0; i < Model.listaUsuarios.Count; i++)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => Model.listaUsuarios.ElementAt(i).nombre)
                    @Html.DisplayFor(modelItem => Model.listaUsuarios.ElementAt(i).apellidos)
                    @Html.Raw(" ")
                </td>
                <td>
                    @Html.Raw(" ")
                </td>
                <td>
                    @Html.Raw(" ")
                </td>
            </tr>
        }
    </tbody>

</table>
